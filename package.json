{
  "name": "nxdos",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz",
    "lint": "pnpm nx run-many --all --target=lint",
    "lint:affected": "pnpm nx affected -t lint",
    "test": "pnpm nx run-many --all --target=test --parallel --codeCoverage",
    "test:affected": "pnpm nx affected -t test --parallel --codeCoverage",
    "test:nxdos:docs": "pnpm nx run-many --projects='nxdos-documentation-*' --target=test --parallel --codeCoverage",
    "e2e": "pnpm nx run-many --all --target=e2e",
    "e2e:affected": "pnpm nx affected -t e2e",
    "merge-coverage": "pnpm istanbul-merge --out='tests/coverage/summary/monorepo-coverage.json' 'tests/coverage/**/coverage-final.json' &&  pnpm nyc report --reporter=html --reporter=text --reporter=lcov --reporter=clover --report-dir=tests/coverage/summary/reports --temp-dir=tests/coverage/summary",
    "qa": "pnpm run lint && pnpm run test && pnpm run merge-coverage && pnpm run e2e",
    "qa:skip-cache": "pnpm run lint --skipNxCache && pnpm run test --skipNxCache && pnpm run merge-coverage && pnpm run e2e --skipNxCache",
    "qa:affected": "pnpm run lint:affected && pnpm run test:affected && pnpm run merge-coverage && pnpm run e2e:affected"
  },
  "private": true,
  "dependencies": {
